{
  "name": "pkg-name",
  "version": "0.0.1",
  "packageManager": "pnpm@7.12.2",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
